{
  "title": "Mustache Sharp Core",
  "description": "A extension of the mustache text template engine for .NET.",
  "authors": ["truncon", "kwilliamssbs"],
  "packOptions": {
    "projectUrl": "https://github.com/SunBrandingSolutions/mustache-sharp",
    "licenseUrl": "https://raw.githubusercontent.com/SunBrandingSolutions/mustache-sharp/master/UNLICENSE.txt",
    "owners": [ "Sun Branding Solutions" ],
    "summary": "An implementation of Mustache in .NET Core.",
    "repository": {
      "type": "git",
      "url": "https://github.com/SunBrandingSolutions/mustache-sharp"
    },
    "tags": [ "mustache", "handlebars.js", "text", "generation", "building", "template" ]
  },
  "version": "1.0.1-*",

  "buildOptions": {
    "embed": {
      "include": [ "Properties/Resources.resx" ]
    },
    "nowarn": [ "1591" ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },

  "frameworks": {
    "netstandard1.3": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Data.Common": "4.1.0",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Text.Encodings.Web": "4.0.0"
      }
    },
    "net452": { }
  }
}
